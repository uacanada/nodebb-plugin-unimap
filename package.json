{
  "name": "nodebb-plugin-unimap",
  "version": "0.0.3",
  "description": "Interactive map plugin for NodeBB forums. Enhance user engagement by allowing location additions and explore monetization opportunities.",
  "main": "lib/backend/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/uacanada/nodebb-plugin-unimap"
  },
  "keywords": [
    "NodeBB",
    "plugin",
    "unimap",
    "Ukraine",
    "map",
    "leaflet",
    "openstreetmap"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "licenseFile": "LICENSE",
  "bugs": {
    "url": "https://github.com/uacanada/nodebb-plugin-unimap/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-angular": "17.6.3",
    "eslint": "8.40.0",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "resolve-url-loader": "^5.0.0"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "leaflet-contextmenu": "^1.4.0",
    "leaflet-control-geocoder": "^2.4.0",
